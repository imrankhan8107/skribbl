{
  "name": "skribbl",
  "version": "1.0.0",
  "description": "Pictionary game in which users have 2 roles of a drawer and a guesser, the drawer draws the selected word on canvas so that the guessers can guess the word.",
  "private": true,
  "workspaces": [
    "frontend/",
    "backend/"
  ],
  "scripts": {
    "build": "yarn workspace @skribbl/frontend build",
    "pb": "yarn workspace @skribbl/frontend dev",
    "dev": "yarn workspace @skribbl/backend dev",
    "start": "yarn workspace @skribbl/backend start",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Imran Khan",
  "license": "ISC",
  "dependencies": {
    "@socket.io/admin-ui": "^0.5.1",
    "cookie-parser": "^1.4.6",
    "fabricjs-react": "^1.2.1",
    "parcel": "^2.10.3",
    "short-uuid": "^4.2.2"
  }
}
